<Window x:Class="IikoPaymentPlugin.View.BarCodeScanWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IikoPaymentPlugin.View"
        mc:Ignorable="d"
        Title="BarCodeScanWindow" Height="200" Width="400"
        Loaded="Window_Loaded" Deactivated="Window_Deactivated">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox x:Name="barCodeTB" Width="150" Margin="0 10"/>
            <Button Content="OK" Width="100"/>
        </StackPanel>
    </Grid>
</Window>
